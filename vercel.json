{
    "functions":{
      "api/**":{
          "includeFiles": "requirements.txt",
          "runtime": "python3.11"
      }  
    },
    "rewrites":[
        {
            "source": "/(.*)",
            "destination":"/api/main.py"
        },
        {
            "source":"/(.*)",
            "destination": "/"
        }
    ]
}
